{
    "name": "Figma Dark UI",
    "version": "1.0.1",
    "description": "Dark UI extension for Figma Webapp",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["https://www.figma.com/*"],
        "css": ["content.css"],
        "js": ["script.js"]
    }],
    "icons": {
        "16": "DarkUIFigmaLogo16.png",
        "48": "DarkUIFigmaLogo48.png",
        "128": "DarkUIFigmaLogo128.png"
    },

    "web_accessible_resources": [
        "content.css"
    ]
}